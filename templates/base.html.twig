					</button>
					<div class=" px-md-5 collapse navbar-collapse" id="navbarSupportedContent">
						<ul class="navbar-nav me-auto mb-2 mb-lg-0">
							<li class="nav-item px-md-2">
								<a class="nav-link" href="{{ path('app_home') }}">Accueil</a>
							</li>
							<li class="nav-item px-md-2">
								<a class="nav-link" href="{{ path('app_home') }}#decouvrir">Découvrir</a>
							</li>
							{% if app.user %}
								<li class="nav-item px-md-2">
									<a class="nav-link" href="{{ path('app_cours', {userId: app.user.id}) }}">Cours</a>
								</li>
							{% endif %}
							<li class="nav-item px-md-2">
								<a class="nav-link" href="{{ path('app_contact') }}">Contact</a>
							</li>
						</ul>
					</div>
					<div class="d-flex align-items-center">
						<div class="dropdown a ">
							<a class="a dropdown-toggle d-flex align-items-center hidden-arrow" href="#" role="button" aria-expanded="false" data-bs-toggle="dropdown">
								{% if app.user %}
									{% if app.user.photo is not null %}
										<img class="imageAvatar image" src="{{ asset('uploads/brochures/' ~ app.user.photo.url ~ '') }}"/>
									{% else %}
										<img class="imageAvatar" src="{{ asset('assets/img/iconprofile.svg') }}"/>

									{% endif %}

								{% else %}
									<img class="imageAvatar" src="{{ asset('assets/img/iconprofile.svg') }}"/>

								{% endif %}
							</a>
							<ul class="dropdown-menu dropdown-menu-end ">
