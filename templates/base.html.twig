									</li>

									<li>
										<a class="dropdown-item a" href="{{ path('app_logout') }}">Déconnection</a>
									</li>
								{% else %}
									<li>
										<a class="dropdown-item a" href="{{ path('app_register') }}">Inscripion</a>
									</li>
									<li>
										<a class="dropdown-item a" href="{{ path('app_login') }}">connection</a>
									</li>
								{% endif %}
							</ul>
						</div>
					</div>
				</div>
